{ "background": {
  "persistent": false,
  "scripts": ["background.js"]
  },
  "content_scripts": [
  	{
  		"matches":[
  			"*://twitter.com/compose/tweet?text*"
  		],
  		"js":["content.js"]
  	}
  ],
  "manifest_version": 2,

  "name": "Auto Tweet",
  "description": "Otomatik twit atma",
  "version": "2.2.0",
  "icons": { 
    "128": "icon_128.png" 
  },
  
  "permissions": ["contextMenus", "activeTab", "cookies", "*://twitter.com/*"],
  

  "browser_action":{
  	"default_icon": "icon.png",
  	"default_title": "SahinS Extension"
  }

}
