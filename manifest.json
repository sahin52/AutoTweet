{ "background": {
  "persistent": false,
  "scripts": ["background.js"]
  },
  "manifest_version": 2,

  "name": "Auto Tweet",
  "description": "Otomatik twit atma",
  "version": "2.2.0",
  "icons": { 
    "128": "icon_128.png" 
  },
  
  "permissions": ["contextMenus", "activeTab", "cookies", "*://twitter.com/*"],
  

  "browser_action":{
  	"default_icon": "icon.png",
  	"default_popup": "popup.html",
  	"default_title": "SahinS Extension"
  }

}
